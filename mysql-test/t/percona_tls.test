--echo #
--echo # Percona tests for TLS
--echo #

--echo # Test that neither TLSv1 or TLSv1.1 are allowed

# Test requires TLSv1 and TLSv1.1
--source include/not_min_protocol_tlsv12.inc

--source include/shutdown_mysqld.inc

--let MYSQLD_LOG= $MYSQL_TMP_DIR/server.log
--error 1
--exec $MYSQLD --log-error=$MYSQLD_LOG --tls-version="TLSv1,TLSv1.1"

--let SEARCH_FILE= $MYSQLD_LOG
--let SEARCH_PATTERN= Option --tls-version or --admin-tls-version is set to an invalid value TLSv1,TLSv1.1.
--source include/search_pattern.inc

# Cleanup
--remove_file $MYSQLD_LOG
--source include/start_mysqld.inc
