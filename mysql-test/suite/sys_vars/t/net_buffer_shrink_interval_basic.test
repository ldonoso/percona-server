--source include/restart_mysqld.inc
--echo See what is default size of the net_buffer_length
show variables like 'net_buffer_length';
--echo See the buffer size of the default connectcion
show status like 'net_buffer_length';

--echo Check basic net_buffer_shrink_interval variable functionality
show variables like 'net_buffer_shrink_interval';
set global net_buffer_shrink_interval=1;
show variables like 'net_buffer_shrink_interval';
set global net_buffer_shrink_interval=40000000;
show variables like 'net_buffer_shrink_interval';
--error ER_GLOBAL_VARIABLE
set session net_buffer_shrink_interval=0;
show variables like 'net_buffer_shrink_interval';
set global net_buffer_shrink_interval=0;
show variables like 'net_buffer_shrink_interval';

--echo See the buffer size of the default connection
show status like 'net_buffer_length';

--echo Reduce initial buffer size for new connections
set global net_buffer_length=4096;
show global variables like 'net_buffer_length';

--echo See the total usage before connection has been created
show global status like 'net_buffer_length';

set global net_buffer_shrink_interval=2;

CREATE TABLE t1(data LONGBLOB) ENGINE=INNODB;

#Create con_int1
connect (con_int1,localhost,root,,);
connection con_int1;

--echo Check con_int1 buffer size, should be 4096
show status like 'net_buffer_length';

#Create con_int2
connect (con_int2,localhost,root,,);
connection con_int2;

--echo Check con_int2 buffer size, should be 4096
show status like 'net_buffer_length';

--echo See the total usage after connections have been created
show global status like 'net_buffer_length';

#Insert big value(8096 bytes) to increase buffers in both connections
connection con_int1;

INSERT INTO t1 (data) VALUES ("







");
--echo see con1 buffer after grow
show status like 'net_buffer_length';

connection con_int2;

INSERT INTO t1 (data) VALUES ("







");
--echo see con2 buffer after grow
show status like 'net_buffer_length';
--echo See total usage after buffer has been grown
show global status like 'net_buffer_length';

--echo Wait for first shrink check interval expire
SELECT SLEEP(3);

--echo Shrink check triggered by queries, so trigger it in both threads
show status like 'net_buffer_length';
connection con_int1;

show status like 'net_buffer_length';
connection con_int2;

--echo Wait for second shrink check interval expire
SELECT SLEEP(3);

--echo First query to trigger check, second to see shrinked buffer
show status like 'net_buffer_length';
show status like 'net_buffer_length';

connection con_int1;

--echo First query to trigger check, second to see shrinked buffer
show status like 'net_buffer_length';
show status like 'net_buffer_length';

--echo See what is the global usage after both buffers have bee shrinked
show global status like 'net_buffer_length';

connection default;

--echo Check default connections size
show status like 'net_buffer_length';

disconnect con_int1;
disconnect con_int2;

--echo By some reason above disconnect doesn't work and the size is 8K more then expected
show global status like 'net_buffer_length';

DROP TABLE t1;

set global net_buffer_length=default;
set global net_buffer_shrink_interval=default;
